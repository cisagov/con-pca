                            </tr>
                            <tr>
                                <td>Dates&colon;</td>
                                <td>{{ target_cycle.start_date|date:"M d, Y" }} - {{ target_cycle.end_date|date:"M d, Y" }}</td>
                            </tr>
                            <tr>
                                <td>Test Location&colon;</td>
