        <article id="pageSix">
            <div class="row">
                <div class="col-12">
                    <div id="successOverview">
                        <h4><small>PHISHING SUCCESS OVERVIEW</small></h4>
                        <svg class="chart" width="800" height="210" aria-labelledby="title desc" role="img">
                            <title id="title">STATISTICS BY LEVEL</title>
                            <desc id="units-achieved">0; 50; 100; 150; 200; 250; 300; 350; 400;</desc>
                            <g class="measure-line">
                                <line x1="30" y1="15" x2="800" y2="15" style="stroke:#BAD6E4;stroke-width:2"/>
                                <line x1="30" y1="55" x2="800" y2="55" style="stroke:#BAD6E4;stroke-width:2"/>
                                <line x1="30" y1="95" x2="800" y2="95" style="stroke:#BAD6E4;stroke-width:2"/>
                                <line x1="40" y1="95" x2="790" y2="95" style="stroke:#FDC010;stroke-width:3"/>
                                <circle cx="35" cy="95" style="fill: #FDC010;" r="5"/>
                                <circle cx="795" cy="95" style="fill: #FDC010;" r="5"/>
                                <text x="0" y="15" class="caption" dy=".35em">100</text>
                                <text x="8" y="55" class="caption" dy=".35em">50</text>
                                <text x="15" y="95" class="caption" dy=".35em">0</text>
                            </g>
